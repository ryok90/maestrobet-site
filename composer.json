{
    "name": "maestrobet/maestrobet",
    "description": "MaestroBet Site",
    "require": {
        "zendframework/zendframework": "^3.0",
        "doctrine/doctrine-orm-module": "^2.1",
        "zendframework/zend-session": "^2.9",
        "api-skeletons/zf-oauth2-doctrine": "^3.1",
        "zfcampus/zf-apigility": "^1.5",
        "zfcampus/zf-development-mode": "^3.2",
        "zf-commons/zfc-rbac": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Usuario\\": "module/Usuario/src/",
            "Financeiro\\": "module/Financeiro/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "zfcampus/zf-apigility-admin": "~1.0",
        "zend/zend-studio-development-mode": "^1.0",
        "zfcampus/zf-asset-manager": "^1.2"
    },
    "scripts": {
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "run-doctrine": [
            "doctrine-module orm:clear-cache:metadata",
            "doctrine-module orm:clear-cache:query",
            "doctrine-module orm:clear-cache:result",
            "doctrine-module orm:schema-tool:update --force"
        ]
    }
}
